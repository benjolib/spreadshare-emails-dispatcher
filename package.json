{
  "name": "spreadshare-email-dispatcher",
  "version": "0.1.0",
  "description": "A aws lambda email dispatcher service for spreadshare",
  "author": "Shashank Tomar (shashank004@gmail.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+git@bitbucket.org:spreadshare/spreadshare-email-dispatcher.git"
  },
  "keywords": [
    "serverless",
    "nodejs",
    "webpack",
    "babel",
    "aws-lambda"
  ],
  "scripts": {
    "dev": "sls offline start",
    "deploy:dev": "sls deploy -s dev",
    "deploy:stage": "sls deploy -s stage",
    "deploy:prod": "sls deploy -s prod",
    "invoke:local": "sls invoke local --function",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint:fix": "npm run lint -- --fix",
    "logs": "sls logs -s dev -t -f",
    "flowtyped": "flow-typed install",
    "test": "jest",
    "test:coverage": "npm test -- --coverage --collectCoverageFrom=src/**/*.{js,jsx}",
    "test:watch": "npm test -- --watch",
    "test:all": "npm run lint && npm run typecheck && npm run test",
    "typecheck": "flow"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.5.1",
    "jest": "^21.2.1",
    "minimist": "^1.2.0",
    "prettier": "^1.14.0",
    "serverless-offline": "^3.20.3",
    "serverless-webpack": "5.1.0",
    "webpack": "4.2.0",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "babel-runtime": "^6.26.0",
    "http-errors": "^1.6.3",
    "middy": "^0.15.7",
    "mjml": "^4.1.1",
    "nodemailer": "^4.6.7",
    "nodemailer-mailgun-transport": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "source-map-support": "^0.4.18"
  }
}
